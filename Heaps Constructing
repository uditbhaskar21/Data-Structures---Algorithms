public static void main(String[] args) {
        PriorityQueue<Integer> pq = new PriorityQueue<>(Collections.reverseOrder());
        int[] ranks = {10,22,45,86,3,35,19};
        for (int val:ranks){
            pq.add(val);
        }
        while (pq.size()>0){
            System.out.println(pq.peek());
            pq.remove();
        }
    }
