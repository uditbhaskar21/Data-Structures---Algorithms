import java.util.Scanner;

public class printIncreasing {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n= sc.nextInt();
        printIncrease(n);

    }
    public static void printIncrease(int n){
        if (n==0){
            return;
        }
        printIncrease(n-1);
        System.out.println(n);

    }
}
